{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/generate-poem.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/generate-poem",
      "dest": "/api/generate-poem.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
